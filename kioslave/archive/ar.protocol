[Protocol]
exec=kio_archive
protocol=ar
archiveMimetype=application/x-archive,application/x-rar
input=filesystem
output=filesystem
listing=Name,Type,Size,Date,Access,Owner,Group,Link
reading=true

# writing/makedir/deleting/moving only work if the proprietary rar command is installed
writing=true
makedir=true
deleting=true
opening=true
moving=true
source=true
Icon=package-x-generic
Class=:local
